{
  "version": "0.2.0",
  "configurations": [
  {
    "command": "npm start",
    "name": "Run npm start",
    "request": "launch",
    "type": "node-terminal"
  },
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector"
    }
  ]
}
